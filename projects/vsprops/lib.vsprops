<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="VSProps Lib"
	>
	<Tool
		Name="VCLibrarianTool"
    OutputFile="$(OutDir)/libs/$(ProjectName)-$(_Z_PLTFRM_)$(_Z_ARCH_).lib"
    ProgramDatabaseFile="$(OutDir)/pdb/$(ProjectName)-$(_Z_PLTFRM_)$(_Z_ARCH_).pdb"
